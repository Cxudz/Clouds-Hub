--Clouds Test
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Clouds Hub", HidePremium = true, IntroEnabled = false, SaveConfig = true, ConfigFolder = "CloudsConfig"})

-- Information
local InformationTab = Window:MakeTab({
	Name = "Information",
	Icon = "rbxassetid://7422600859",
	PremiumOnly = false
})

InformationTab:AddParagraph("Welcome to Clouds Hub!","Clouds Hub Uses a Game ID detection system to see what game you are currentky playing. This is so that your menu can avoid being clutterd and stay clean and organized so that you can have the best and smoothest experience possible, Enjoy!")
InformationTab:AddParagraph("IMPORTANT!","I did not make any of the code used in this hub, I just put it together. If you are the owner of any of the scripts in this hub DM me at Clouds#2798 and I will take it off the hub")

-- Universal
local UniversalTab = Window:MakeTab({
	Name = "Universal",
	Icon = "rbxassetid://7422600859",
	PremiumOnly = false
})

UniversalTab:AddParagraph("Universal Tab Information","Here you can find scripts that will work in almost all games.")

UniversalTab:AddButton({
	Name = "Owl Hub",
	Callback = function()
      		print("Owl Hub button pressed")
              loadstring(game:HttpGet("https://raw.githubusercontent.com/CriShoux/OwlHub/master/OwlHub.txt"))();
              OrionLib:MakeNotification({
                Name = "Success!",
                Content = "Owl Hub has successfully been executed",
                Time = 5
            })
  	end    
})

UniversalTab:AddButton({
	Name = "Infinite Yield",
	Callback = function()
      		print("Infinite Yield button pressed")
              loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
              OrionLib:MakeNotification({
                Name = "Success!",
                Content = "Infinite Yield has successfully been executed",
                Time = 5
            })
  	end    
})

UniversalTab:AddButton({
	Name = "Nullware V3 [Animation Player]",
	Callback = function()
      		print("Nullware V3 button pressed")
              getgenv().Theme = "Purple"
              loadstring(game:HttpGet("https://gist.githubusercontent.com/M6HqVBcddw2qaN4s/2d722888a388017c18028cd434c43a25/raw/dcccf1027fe4b90780e47767aaf584389c9d7771/EULma3fU90PUOKUn?identifier=".. (function()local a=""for b=1,256 do local c=math.random(1,3)a=a..string.char(c==1 and math.random(48,57)or c==2 and math.random(97,122)or c==3 and math.random(65,90))end;return a end)()))()
              OrionLib:MakeNotification({
                Name = "Success!",
                Content = "Nullware V3 has successfully been executed",
                Time = 5
            })
  	end    
})

UniversalTab:AddButton({
	Name = "Chat Spy",
	Callback = function()
      		print("Chat Spy button pressed")
              loadstring(game:HttpGet("https://raw.githubusercontent.com/Cxudz/Clouds-Hub/main/Chat%20Spy%20%5BNOT%20MINE%5D"))()
              OrionLib:MakeNotification({
                Name = "Success!",
                Content = "Chat Spy has successfully been executed",
                Time = 5
            })
  	end    
})

--Combat Warriors
if game.PlaceId == 4282985734 then
    OrionLib:MakeNotification({
        Name = "Found Game Combat Warriors",
        Content = "Loaded Tab specifically for Combat Warriors",
        Image = false,
        Time = 5
    })
    local CombatWarriorsTab = Window:MakeTab({
        Name = "CW",
        PremiumOnly = false
    })
    CombatWarriorsTab:AddButton({
        Name = "Cheese Menu [NW]",
        Callback = function()
                  print("Cheese Menu button pressed")
                  loadstring(game:HttpGet("https://cheeseskid.tk/shit/combat_warriors.lua",true))()
                  OrionLib:MakeNotification({
                    Name = "Success!",
                    Content = "Cheese Menu has successfully been executed",
                    Time = 5
                })
          end    
    })
    CombatWarriorsTab:AddButton({
        Name = "Project Hook [Free]",
        Callback = function()
                  print("Project Hook button pressed")
                  loadstring(game:HttpGet("https://projecthook.xyz/scripts/free.lua"))()
                  OrionLib:MakeNotification({
                    Name = "Success!",
                    Content = "Project Hook has successfully been executed",
                    Time = 5
                })
          end    
    })
end   


